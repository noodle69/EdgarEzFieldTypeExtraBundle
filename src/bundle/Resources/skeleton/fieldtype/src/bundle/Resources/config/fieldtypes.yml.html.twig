services:
    {{ namespace }}\FieldType\{{ fieldtype_basename }}\Type:
        parent: ezpublish.fieldType
        tags:
            - {name: ezpublish.fieldType, alias: {{ fieldtype_namespace_identifier }}{{ fieldtype_identifier }}}

    {{ namespace }}\FieldType\Mapper\{{ fieldtype_basename }}FormMapper:
        tags:
            - { name: ez.fieldFormMapper.definition, fieldType: {{ fieldtype_namespace_identifier }}{{ fieldtype_identifier }} }
            - { name: ez.fieldFormMapper.value, fieldType: {{ fieldtype_namespace_identifier }}{{ fieldtype_identifier }} }
